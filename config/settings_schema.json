            "pp-privacy-list": {
                "type": "pp-privacy-list",
                "blocks": {
                    "privacy_policy_paragraph_zdqmyE": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Introduction"
                        }
                    },
                    "privacy_policy_paragraph_74zQ3w": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "We value your privacy and are committed to protecting how your personal information is handled. This Privacy Policy outlines how we safeguard your privacy as a user (“you,” “your,” “user(s)”) when you use the Zugu Ring and Zugu application, along with any other websites, mobile or digital applications, or services we may offer that link to this Privacy Policy (collectively referred to as the “Platform”). The Privacy Policy details the types of information we collect and monitor when you use our Platform, as well as how this information is used and shared."
                        }
                    },
                    "privacy_policy_paragraph_wTVLxQ": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Your Consent"
                        }
                    },
                    "privacy_policy_paragraph_khkWxh": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "By accessing or using the Zugu application, including downloading the mobile app, you consent to the collection, use, and disclosure of your personal information as described in this Privacy Policy, or as otherwise required or permitted by applicable law."
                        }
                    },
                    "privacy_policy_paragraph_eFDHM3": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Scope of this Privacy Policy"
                        }
                    },
                    "privacy_policy_paragraph_F6VtwY": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "This Privacy Policy addresses the collection of personally identifiable information (\"personal information\") when you access or use the Zugu Ring and Zugu application. It does not apply to the policies or practices of companies we do not own or control, or to individuals we do not employ."
                        }
                    },
                    "privacy_policy_paragraph_qFWcip": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Personal information refers to details that identify, relate to, describe, or can reasonably be linked to you, either on its own or combined with other information we have. Examples of personal information include your name, contact details, and any information you provide when creating an account or profile on our application."
                        }
                    },
                    "privacy_policy_paragraph_6LYGqg": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "We do not knowingly collect or solicit personal information from individuals under the age of 18, nor do we allow such persons to register for our products or services, including personalized features, product delivery, or feedback on activities within the application."
                        }
                    },
                    "privacy_policy_paragraph_7Cfzzh": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Information That Is Collected Through Our Application"
                        }
                    },
                    "privacy_policy_paragraph_wdzziC": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "We collect the personal information you voluntarily provide when you use our application."
                        }
                    },
                    "privacy_policy_paragraph_XnJNkq": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Upon Sign-Up: When you create a user profile on our Platform, we gather your name, age, gender, email address, and any other relevant information you provide. You may also choose to share additional details such as payment information, location data, or other information relevant to our services and products."
                        }
                    },
                    "privacy_policy_paragraph_GFjzmb": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "While Using Our Application: As you download and use our mobile application, we may collect information about your mobile device (including device and application identifiers), the referring web page, pages visited, IP address, and location (depending on the permissions you have granted). When we collect usage data from the application, it is used only in aggregate form and not in a way that identifies you personally."
                        }
                    },
                    "privacy_policy_paragraph_TeXQMe": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Use of Our Smart Ring: If you use the Zugu Ring, you acknowledge that any information, data, or readings from the Zugu Ring will be automatically uploaded to your user account with us. This data may include heart rate, device identifiers, and location data (depending on permissions granted). If these automatically collected readings can identify you personally, they are considered personal information under this Privacy Policy."
                        }
                    },
                    "privacy_policy_paragraph_eRYiWj": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Social Media Integration"
                        }
                    },
                    "privacy_policy_paragraph_Kgrd7h": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "The Zugu Ring application has integrated social features (e.g., sharing specific content to social media), and we will only share content according to your preferences and upon triggering designated conditions, such as emergency notifications or pre-set permissions."
                        }
                    },
                    "privacy_policy_paragraph_FwggfE": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "How We Use Your Information"
                        }
                    },
                    "privacy_policy_paragraph_hxKyLf": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Data collected from the Zugu Ring is uploaded to your user account on the Zugu application and is used in aggregate to provide product features and improve the user experience. Personal information is used solely for the intended purposes of the Zugu Ring and application experience."
                        }
                    },
                    "privacy_policy_paragraph_ciVAtY": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "User Consent and Opt-Out Options"
                        }
                    },
                    "privacy_policy_paragraph_mWk4kY": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "You have control over certain data collection features or app permissions, such as location tracking, social media posting, or data collection. You can opt out of specific features or revoke app permissions at any time through the app settings."
                        }
                    },
                    "privacy_policy_paragraph_J4EeJ7": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Data Retention and Deletion"
                        }
                    },
                    "privacy_policy_paragraph_BWAe7i": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Personal data collected through the Zugu Ring application will be retained only as long as necessary for the purposes outlined. You may delete your account or request data deletion at any time by contacting DeathCaseSupport@zugucase.com."
                        }
                    },
                    "privacy_policy_paragraph_r7NBpW": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Security of Your Information"
                        }
                    },
                    "privacy_policy_paragraph_TrtKEr": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "We use secure encryption and access restrictions to protect your personal information, ensuring that it is accessed only by authorized personnel or partners necessary to operate the Zugu Ring and application. Your trust is important to us, and we take security seriously to safeguard your data."
                        }
                    },
                    "privacy_policy_paragraph_whBFfX": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "Changes to This Policy"
                        }
                    },
                    "privacy_policy_paragraph_TQLVVF": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "We may update this Privacy Policy periodically to address changes in the law, our data collection and usage practices, service features, or technological advancements. Your continued use of the Platform, services, and products after any updates to this Privacy Policy will indicate your acceptance and consent to those changes."
                        }
                    },
                    "privacy_policy_paragraph_mFxVby": {
                        "type": "privacy_policy_paragraph",
                        "settings": {
                            "text": "© 2024 Zugu, Inc. All Rights Reserved."
                        }
                    }
                },
